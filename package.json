{
  "name": "md_cascading_kpi",
  "version": "0.0.1",
  "description": "Progetto Cascading KPI",
  "main": "server.js",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds-dk": "^7",
    "@sap/cds-hana": "^2.0.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.7",
    "@sap/xssec": "^3.6.1",
    "express": "^4.18.2",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@sap/cds": "^8.1.0",
    "@sap/cds-dk": "^7"
  },
  "scripts": {
    "start": "cds-serve",
    "build": "cds build --production",
    "prepare_deploy": "npm install @sap/cds && mbt build "
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "hana",
        "model": [
          "db",
          "srv"
        ],
        "pool": {
          "max": 300,
          "min": 0
        }
      },
      "auth": "jwt"
    }
  }
}
